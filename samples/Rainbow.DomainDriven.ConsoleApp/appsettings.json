{
    "Domain": {
        "Local": {
            "EventBufferSize": 1048576,
            "CommandBufferSize": 1048576,
            "MaxSequencer": 4000
        },
        "MongoDB": {
            "ConnectionString": "mongodb://127.0.0.1:27017",
            "Database": "rainbow_sample"
        },
        "Sqlite": {
            "ConnectionString": "Data Source=${workspaceRoot}/test1.db"
        }
    },
    "Logging": {
        "PathFormat": "Logs/log-{Date}.txt",
        "LogLevel": {
            "System": "Error",
            "Microsoft.EntityFrameworkCore": "Error",
            "Rainbow.DomainDriven": "Error",
            "Rainbow.DomainDriven.Command": "Error",
            "Rainbow.DomainDriven.Event": "Error",
            "Rainbow.DomainDriven.Queue": "Error",
            "Rainbow.DomainDriven.ConsoleApp": "Information"
        }
    }
}